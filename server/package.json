{
  "name": "stargazer-server",
  "version": "0.0.1",
  "description": "Stargazer Satellite System",
  "author": "TenX",
  "private": false,
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "prettier": "prettier --list-different './index.ts'",
    "prettier:fix": "prettier --write './index.ts'",
    "tslint": "tslint --project tsconfig.json -c tslint.json",
    "test": "npm run prettier && npm run tslint && npm run tsc",
    "dev": "tsc-watch --onSuccess \"babel-node ./build/index.js\"",
    "start": "node ./build/index.js",
    "compile": "tsc && babel build -d build",
    "build": "webpack"
  },
  "devDependencies": {
    "@types/express": "4.16.1",
    "babel-cli": "6.26.0",
    "express": "4.16.4",
    "babel-preset-env": "1.7.0",
    "prettier": "1.16.4",
    "ts-loader": "5.3.3",
    "tsc-watch": "2.1.2",
    "tslint": "5.13.1",
    "tslint-immutable": "5.3.2",
    "typescript": "3.2.4",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1"
  }
}
