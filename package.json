{
  "name": "react-native-stargazer",
  "version": "0.0.36",
  "description": "Stargazer Control System",
  "author": "TenX",
  "private": false,
  "license": "ISC",
  "keywords": [
    "expo",
    "react-native",
    "testing",
    "ui-testing",
    "regression-testing",
    "automated-testing"
  ],
  "main": "./app/build/index.js",
  "types": "./app/build/index.d.ts",
  "bin": {
    "stargazer:run": "./bin/run.sh",
    "stargazer:client": "./bin/client.sh",
    "stargazer:server": "./bin/server.sh",
    "stargazer:build": "./bin/build.sh"
  },
  "scripts": {
    "setup": "npm install && npm run install-packages",
    "install-packages": "cd app && npm install && cd ../client && npm install && cd ../server && npm install",
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "build": "cd client && npm run build",
    "test": "./test.sh",
    "prepublish": "npm run test"
  },
  "dependencies": {
    "serve": "^11.0.0"
  }
}
