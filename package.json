{
  "name": "react-native-stargazer",
  "version": "0.0.5",
  "description": "Stargazer Control System",
  "author": "TenX",
  "private": false,
  "keywords": [],
  "license": "ISC",
  "main": "./app/lib/index.js",
  "types": "./app/lib/index.d.ts",
  "bin": {
    "stargazer:run": "./bin/run.sh",
    "stargazer:client": "./bin/client.sh",
    "stargazer:server": "./bin/server.sh",
    "stargazer:build": "./bin/build.sh"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "build": "cd client && npm run bundle",
    "prepublish": "cd server && npm run build && cd ../app && npm run build",
    "postinstall": "cd app && npm install --production && cd ../client && npm install --production"
  }
}
